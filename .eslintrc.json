{
    "parser": "babel-eslint",
    "extends": "eslint:recommended",

    "env": {
        "es6": true,
        "amd": true,
        "node": true,
        "browser": true,
        "mocha": true
    },

    "globals": {

    },

    "rules": {
        //"no-cond-assign": 2,
        "no-console": [2, { "allow": ["warn", "error"] }],
        //"no-constant-condition": 2,
        //"no-control-regex": 2,
        //"no-debugger": 2,
        //"no-dupe-args": 2,
        //"no-dupe-keys": 2,
        //"no-duplicate-case": 2,
        //"no-empty-character-class": 2,
        //"no-empty": 2,
        //"no-ex-assign": 2,
        //"no-extra-boolean-cast": 2,
        "no-extra-parens": 2, // autofix
        //"no-extra-semi": 2,  // autofix
        //"no-func-assign": 2,
        //"no-inner-declarations": 2,
        //"no-invalid-regexp": 2,
        //"no-irregular-whitespace": 2,
        //"no-obj-calls": 2,
        "no-prototype-builtins": 2,
        //"no-regex-spaces": 2, // autofix
        //"no-sparse-arrays": 2,
        "no-template-curly-in-string": 2,
        //"no-unexpected-multiline": 2,
        //"no-unreachable": 2,
        //"no-unsafe-finally": 2,
        "no-unsafe-negation": 2, // autofix
        //"use-isnan": 2,
        "valid-jsdoc": [2, { "requireReturn": false }],
        //"valid-typeof": 2,
        "accessor-pairs": 2,
        "array-callback-return": 2,
        "block-scoped-var": 2,
        "class-methods-use-this": 2,
        "complexity": 2,
        "consistent-return": 2,
        "curly": 2,
        //"default-case": 2,
        "dot-location": [2, "property"], // autofix
        "dot-notation": 2, // autofix
        "eqeqeq": [2, "always", {"null": "ignore"}],
        "guard-for-in": 2,
        "no-alert": 2,
        "no-caller": 2,
        //"no-case-declarations": 2,
        "no-div-regex": 2,
        "no-else-return": 2,
        "no-empty-function": 2,
        //"no-empty-pattern": 2,
        "no-eq-null": 2,
        "no-eval": 2,
        "no-extend-native": 2,
        "no-extra-bind": 2,
        "no-extra-label": 2,
        //"no-fallthrough": 2,
        //"no-floating-decimal": 2, // autofix
        "no-global-assign": 2,
        //"no-implicit-coercion": 2,
        //"no-implicit-globals": 2,
        "no-implied-eval": 2,
        "no-invalid-this": 2,
        "no-iterator": 2,
        "no-labels": 2,
        "no-lone-blocks": 2,
        "no-loop-func": 2,
        //"no-magic-numbers": 2,
        "no-multi-spaces": 2, // autofix
        //"no-multi-str": 2,
        "no-new-func": 2,
        "no-new-wrappers": 2,
        "no-new": 2,
        "no-octal-escape": 2,
        "no-octal": 2,
        //"no-param-reassign": 2,
        "no-proto": 2,
        "no-redeclare": 2,
        "no-return-assign": 2,
        "no-script-url": 2,
        //"no-self-assign": 2,
        "no-self-compare": 2,
        "no-sequences": 2,
        "no-unmodified-loop-condition": 2,
        "no-useless-call": 2,
        "no-useless-concat": 2,
        "no-useless-escape": 2,
        "no-void": 2,
        "no-warning-comments": [2, { "terms": ["todo"], "location": "start" }],
        "no-with": 2,
        "radix": 2,
        "vars-on-top": 1,
        "yoda": 2,
        "no-delete-var": 2,
        "no-shadow": 2,
        "no-undef-init": 2, // autofix
        "no-undef": 2,
        "no-undefined": 2,
        "no-unused-vars": 2,
        "no-use-before-define": 2,
        "array-bracket-spacing": 2, // autofix
        "block-spacing": 2, // autofix
        "brace-style": [2, "1tbs", { "allowSingleLine": true }],
        "camelcase": 2,
        "comma-dangle": 2, // autofix
        "comma-spacing": 2, // autofix
        "comma-style": 2, // autofix
        "computed-property-spacing": 2, // autofix
        "consistent-this": [2, "self"],
        "eol-last": 2, // autofix
        "func-call-spacing": 2, // autofix
        //"func-names": 2,
        "func-style": 2,
        "indent": [2, 4, {"SwitchCase": 1}], // autofix
        "jsx-quotes": 2, // autofix
        "key-spacing": 2, // autofix
        "keyword-spacing": 2, // autofix
        "line-comment-position": 2,
        //"linebreak-style": 2, // autofix
        "lines-around-comment": [2, {  "beforeLineComment": false, "allowBlockStart": true }], // autofix
        "lines-around-directive": 2,
        "max-depth": 2,
        "max-nested-callbacks": [2, 3],
        "max-params": [2],
        "max-statements": 2,
        "new-cap": 2,
        "new-parens": 2, // autofix
        "newline-after-var": 2,
        "newline-before-return": 2, // autofix
        "newline-per-chained-call": [2, { "ignoreChainWithDepth": 3 }],
        "no-array-constructor": 2,
        "no-bitwise": 2,
        "no-continue": 2,
        "no-lonely-if": 2,
        "no-mixed-operators": 2,
        "no-mixed-spaces-and-tabs": 2,
        "no-multiple-empty-lines": 2, // autofix
        "no-nested-ternary": 2,
        "no-new-object": 2,
        "no-trailing-spaces": [2, { "skipBlankLines": true }], // autofix
//        "no-underscore-dangle": [2, { "allowAfterThis": true, "allowAfterSuper": true }],
        "no-unneeded-ternary": 2,
        "no-whitespace-before-property": 2, // autofix
        "object-curly-newline": ["error", { "multiline": true }], // autofix
        "object-curly-spacing": [2, "always"], // autofix
        "object-property-newline": [2, { "allowMultiplePropertiesPerLine": true }],
        "one-var-declaration-per-line": 2,
        "one-var": [2, "never"],
        "operator-linebreak": [2, "none"],
        "quote-props": [2, "consistent-as-needed"], // autofix
        "quotes": [2, "single"], // autofix
        "require-jsdoc": [2, {
          "require": {
//            "FunctionDeclaration": true,
            "MethodDefinition": true,
            "ClassDeclaration": true
          }
        }],
        "semi-spacing": 2, // autofix
        "semi": 2, // autofix
        "space-before-blocks": 2, // autofix
        "space-before-function-paren": [2, "never"], // autofix
        "space-in-parens": [2, "never"], // autofix
        "space-infix-ops": 2, // autofix
        "space-unary-ops": [2, { "words": true, "nonwords": false }], // autofix
        "spaced-comment": [2, "always"], // autofix
        //"arrow-body-style": [2, "always"],
        "arrow-parens": [2, "as-needed"], // autofix
        "arrow-spacing": 2, // autofix
        "no-duplicate-imports": 2,
        "no-useless-constructor": 2,
        "no-var": 2, // autofix
        "object-shorthand": 2, // autofix
        "prefer-arrow-callback": 2, // autofix
        "prefer-template": 2, // autofix
        "template-curly-spacing": 2 // autofix
    }
}
